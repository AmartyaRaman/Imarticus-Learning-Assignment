{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "functions": {
    "backend/index.js": {
      "runtime": "@vercel/node@2"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ]
}
